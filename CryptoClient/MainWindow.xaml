<Window x:Class="CryptoClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CryptoClient"
        xmlns:v="clr-namespace:CryptoClient.Views"
        xmlns:vm="clr-namespace:CryptoClient.ViewModels"
        mc:Ignorable="d" WindowState="Maximized"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <v:CryptoClientView
                
                DataContext="{Binding CryptoClientViewModel}" />
    </Grid>
</Window>
